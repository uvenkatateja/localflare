{
  "name": "demo-app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:worker\" \"pnpm dev:client\"",
    "dev:worker": "wrangler dev --port 8787",
